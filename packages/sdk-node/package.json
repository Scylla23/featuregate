{
    "name": "@featuregate/node-sdk",
    "version": "0.1.0",
    "private": false,
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "publishConfig": {
        "access": "public"
    },
    "sideEffects": false,
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "lint": "eslint .",
        "typecheck": "tsc --noEmit"
    },
    "devDependencies": {
        "tsup": "^8.0.2",
        "typescript": "^5.4.5"
    },
    "dependencies": {
        "@featuregate/evaluator": "workspace:^"
    }
}